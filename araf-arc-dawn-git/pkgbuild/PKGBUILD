# Maintainer: araf
pkgname=araf-arc-dawn
_destname1="/usr/share/themes/"
pkgver=1.0
pkgrel=01
pkgdesc="Arc themes created for Araf"
arch=('any')
license=('custom')
makedepends=()
depends=()
replaces=()
options=(!strip !emptydirs)
source=("$pkgname.tar.gz")
sha256sums=('04db26d59adfdd893a3b513e2493c31202a0b29da0ac2dcfde4b2df7f95d38ae')
package() {
	install -dm755 ${pkgdir}${_destname1}
	cp -r  ${srcdir}/${pkgname}${_destname1}* ${pkgdir}${_destname1}
}
